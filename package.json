        "@fortawesome/react-fontawesome": "^0.1.4",
        "@reach/router": "^1.2.1",
        "babel-jest": "^23.6.0",
        "gh-pages": "^2.0.1",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-redux": "^6.0.0",
        "react-test-renderer": "^16.7.0",
        "redux": "^4.0.1",
