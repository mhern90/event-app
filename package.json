    "homepage": "https://mhern90.github.io/event-app/",
    "description": "",
    "main": "index.js",
    "scripts": {
        "prebuild": "rm -rf dist",
        "build": "parcel build src/index.html --public-url ./",
        "cypress:open": "cypress open",
        "tailwind:css": "tailwind build src/css/tailwind.src.css -c  tailwind.js -o src/css/tailwind.css",
        "test": "jest --silent",
        "test:update": "jest --silent -u",
        "test:watch": "jest --silent -- --watch",
        "test:coverage": "jest --silent --coverage",
        "format": "prettier --write \"src/**/*.{js,jsx,css,json} \"",
        "lint": "eslint \"**/*.{js,jsx}\"",
        "dev": "npm run tailwind:css && parcel src/index.html",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist"
    },
    "keywords": [],
    "author": "Melinda Stanbery",
    "license": "ISC",
