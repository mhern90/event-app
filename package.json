        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "eslint": "^5.12.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-cypress": "^2.2.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-react": "^7.12.3",
